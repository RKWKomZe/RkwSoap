{
    "name": "rkw/rkw-soap",
    "type": "typo3-cms-extension",
    "description": "Extension for a connecting to TYPO3 via SOAP",
    "homepage": "https://www.rkw-kompetenzzentrum.de",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "RKW", "SOAP"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-soap" : "*",
        "typo3/cms-core": "~9.5.0 || ~10.4.0",
		"madj2k/t3-core-extended": "~9.5.4 || ~10.4.0"
	},
    "conflict": {
        "rkw/rkw-order": "*"
	},
    "suggest": {
        "rkw/rkw-basics": "~9.5.0 || ~10.4.0",
        "madj2k/t3-postmaster": "~9.5.0 || ~10.4.0",
        "rkw/rkw-shop": "~9.5.0 || ~10.4.0",
        "madj2k/t3-fe-register": "~9.5.0 || ~10.4.0"
    },
    "autoload": {
        "psr-4": {
            "RKW\\RkwSoap\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "rkw_soap"
        }
    }
}
